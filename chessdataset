   "source": [
    "x = len(df['winner'])\n",
    "\n",
    "def winner_rating_diff():\n",
    "    for i in range(x):\n",
    "        if df.winner[i] == 'white':\n",
    "            df['wrd'] = df['white_rating'] - df['black_rating']\n",
    "        elif df.winner[i] == 'black':\n",
    "            df['wrd'] = df['black_rating'] - df['white_rating']\n",
    "        else: \n",
    "            df['wrd'] = 'Null'\n",
    "        \n",
    "winner_rating_diff()"
   ]
  },
