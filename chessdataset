    "X_turns = X[\"turns\"]\n",
    "Q1 = X_turns.quantile(0.25)\n",
    "Q3 = X_turns.quantile(0.75)\n",
    "IQR = Q3-Q1\n",
    "alt_sinir = Q1 - 1.5*IQR\n",
    "ust_sinir = Q3 + 1.5*IQR\n",
    "tum_aykiriliklar = (X_turns < alt_sinir) | (X_turns > ust_sinir)\n",
    "X_turns[tum_aykiriliklar] = X_turns.mean()"
   ]
  },
